{
  "name": "Cancel Sponsorship Mapping",
  "model": "recurring.contract",
  "mapping": {
    "Commitment_ID": {
      "field": "global_id"
    },
    "CompassNeedKey": {
      "field": "child_id.local_id",
      "search_relational_record": true
    },
    "CorrespondenceEndDate": {
      "field": "end_date"
    },
    "CorrespondentCorrespondenceLanguage": {
      "field": null
    },
    "CorrespondentSupporterGlobalID": {
      "field": null
    },
    "FinalCommitmentOfLine": {
      "to_json_conversion": "''"
    },
    "HoldID": {
      "field": "hold_id.hold_id",
      "allow_relational_creation": true
    },
    "HoldType": {
      "field": null,
      "to_json_conversion": "'Sponsor Cancel Hold'"
    },
    "PrimaryHoldOwner": {
      "field": "write_uid.name",
      "search_relational_record": true
    },
    "SecondaryHoldOwner": {
      "field": "create_uid.name",
      "search_relational_record": true
    },
    "SponsorCorrespondenceLanguage": {
      "field": "reading_language.name",
      "search_relational_record": true
    },
    "SponsorSupporterGlobalID": {
      "field": "correspondent_id.global_id",
      "search_relational_record": true
    },
    "Beneficiary_GlobalID": {
      "field": "child_id.global_id",
      "search_relational_record": true
    },
    "HoldExpirationDate": {
      "field": "hold_expiration_date",
      "to_json_conversion": "str(odoo_value).replace(' ', 'T') + 'Z'"
    },
    "Channel_Name": {
      "field": null
    },
    "LinkType": {
      "field": null
    },
    "DelinkType": {
      "to_json_conversion": "'Sponsor-requested Cancellation'"
    },
    "ParentCommitmentID": {
      "field": "parent_id.global_id"
    },
    "CommitmentEndDate": {
      "field": "end_date"
    },
    "CorrespondentGlobalPartnerID": {
      "to_json_conversion": "config.get('connect_gpid')"
    },
    "IsSponsorCorrespondent": {
      "to_json_conversion": "True"
    },
    "SponsorGlobalPartnerID": {
      "to_json_conversion": "config.get('connect_gpid')"
    },
    "GlobalPartner_ID": {
      "to_json_conversion": "config.get('connect_gpid')"
    }
  }
}