    "HoldExpirationDate": {
      "field": "hold_expiration_date"
    },
    "Channel_Name": {
      "field": null
    },
    "LinkType": {
      "field": null
    },
    "DelinkType": {
      "field": "return 'Sponsor-requested Cancellation'"
    },
    "ParentCommitmentID": {
      "field": "parent_id.global_id",
      "to_json_conversion": "lambda parent: parent or None"
    },
    "CommitmentEndDate": {
      "field": "end_date",
      "search_relational_record": true
    },
    "CorrespondentGlobalPartnerID": {
      "to_json_conversion": "return 'CH'"
    },
    "IsSponsorCorrespondent": {
      "to_json_conversion": "return True"
    },
    "SponsorGlobalPartnerID": {
      "to_json_conversion": "return 'CH'"
    },
    "GlobalPartner_ID": {
      "to_json_conversion": "return 'CH'"
    }
