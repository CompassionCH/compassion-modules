{
  "name": "Upsert Partner Mapping",
  "model": "res.partner",
  "mapping": {
    "GlobalID": {
      "field": "global_id"
    },
    "GPID": {
      "field": "ref",
      "to_json_conversion": "'65-' + odoo_value"
    },
    "Gender": {
      "field": "gmc_gender"
    },
    "MandatoryReviewRequired": {
      "field": "mandatory_review"
    },
    "PreferredName": {
      "field": "preferred_name"
    },
    "CorrespondenceDeliveryPreference": {
      "field": "send_original",
      "to_json_conversion": "'Physical' if odoo_value else 'Digital'"
    },
    "FirstName": {
      "field": "firstname"
    },
    "LastName": {
      "field": "lastname"
    },
    "GlobalPartner": {
      "to_json_conversion": "config.get('connect_gpname')"
    },
    "Status": {
      "to_json_conversion": "'Active'"
    }
  }
}