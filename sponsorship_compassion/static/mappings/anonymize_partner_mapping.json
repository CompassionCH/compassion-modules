{
  "name": "Anonymize Partner Mapping",
  "model": "res.partner",
  "mapping": {
    "GlobalSupporterID": {
      "field": "global_id"
    },
    "GP_SupporterID": {
      "field": "ref",
      "to_json_conversion": "lambda ref: '65-' + ref"
    },
    "GP_ID": {
      "to_json_conversion": "return 'CH'"
    },
    "DataProtection_Type": {
      "to_json_conversion": "return 'ForgetMe'"
    }
  }
}