<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="1">
        <!-- Default stages of mass mailing campaigns -->
        <record id="notification_stage_1" model="firebase.notification.stage">
            <field name="name">Draft</field>
            <field name="sequence">10</field>
        </record>
        <record id="notification_stage_2" model="firebase.notification.stage">
            <field name="name">Job queue</field>
            <field name="sequence">20</field>
        </record>
        <record id="notification_stage_3" model="firebase.notification.stage">
            <field name="name">Sending</field>
            <field name="sequence">30</field>
        </record>
        <record id="notification_stage_4" model="firebase.notification.stage">
            <field name="name">Sent</field>
            <field name="sequence">40</field>
        </record>
    </data>
</odoo>
