<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="True">
    	<!--
            Resource: account.analytic.account
        -->
        <record id="account_analytic_account_stand" model="account.analytic.account">
            <field name="name">Stand</field>
            <field name="type">normal</field>
            <field name="code">AA1181</field>
            <field name="parent_id" search="[('name', '=', 'Events')]"/>
        </record>
        <record id="account_analytic_account_pres" model="account.analytic.account">
            <field name="name">Presentation</field>
            <field name="type">normal</field>
            <field name="code">AA1182</field>
            <field name="parent_id" search="[('name', '=', 'Events')]"/>
        </record>
        <record id="account_analytic_account_meet" model="account.analytic.account">
            <field name="name">Meeting</field>
            <field name="type">normal</field>
            <field name="code">AA1183</field>
            <field name="parent_id" search="[('name', '=', 'Events')]"/>
        </record>
        <record id="account_analytic_account_sport" model="account.analytic.account">
            <field name="name">Sport</field>
            <field name="type">normal</field>
            <field name="code">AA1184</field>
            <field name="parent_id" search="[('name', '=', 'Events')]"/>
        </record>
        
        <!--
            Resource: ir.sequence
        -->
        <record forcecreate="1" id="aa_seq_type" model="ir.sequence.type">
            <field name="name">Analytic Account Sequence Type</field>
            <field name="code">AASEQ</field>
        </record>
        <record forcecreate="1" id="aa_sequence" model="ir.sequence">
            <field name="name">Analytic account codes</field>
            <field name="code">AASEQ</field>
            <field name="padding">4</field>
            <field name="prefix">AA%(y)s</field>
            <field name="number_next_actual">200</field>
        </record>
    </data>
</openerp>