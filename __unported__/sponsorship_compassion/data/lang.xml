<?xml version='1.0' encoding='UTF-8'?>
<openerp>
  <data noupdate="1">
	<!-- install fr_CH -->
    <record model="base.language.install" id="install_fr_ch">
        <field name="lang">fr_CH</field>
        <field name="state">done</field>
        <field name="overwrite" eval="1"/>
    </record>
    <function
        model="base.language.install"
        name="lang_install"
        eval="[[ref('install_fr_ch')]]"
	/>
	
