{
  "name": "Hold Creation",
  "model": "compassion.intervention.hold.wizard",
  "mapping": {
    "Intervention_ID": {
      "field": "intervention_id.intervention_id",
      "allow_relational_creation": false
    },
    "HoldID": {
      "field": "hold_id"
    },
    "ExpirationDate": {
      "field": "expiration_date"
    },
    "HoldAmount": {
      "field": "hold_amount",
      "to_json_conversion": "str(odoo_value)"
    },
    "NextYearOptIn": {
      "field": "next_year_opt_in"
    },
    "PrimaryOwner": {
      "field": "user_id.name",
      "search_relational_record": true
    },
    "SecondaryOwner": {
      "field": "secondary_owner"
    },
    "ServiceLevelAgreement": {
      "field": "service_level"
    },
    "GlobalPartner_ID": {
      "to_json_conversion": "gpid"
    }
  }
}
