<?xml version="1.0"?>
<odoo>
    <record id="view_hold_crm_tree" model="ir.ui.view">
        <field name="name">compassion.hold.crm.tree</field>
        <field name="model">compassion.hold</field>
        <field name="inherit_id" ref="crm_compassion.view_hold_crm_tree"/>
        <field name="arch" type="xml">
            <field name="event_id" position="after">
                <field name="booked_by_phone_number"/>
                <field name="booked_by_sms_at"/>
            </field>
        </field>
    </record>
    <record id="view_hold_crm_form" model="ir.ui.view">
        <field name="name">compassion.hold.crm.form</field>
        <field name="model">compassion.hold</field>
        <field name="inherit_id" ref="crm_compassion.view_hold_crm_form"/>
        <field name="arch" type="xml">
            <field name="channel" position="after">
                <field name="booked_by_phone_number" readonly="1"/>
                <field name="booked_by_sms_at" readonly="1"/>
            </field>
        </field>
    </record>
</odoo>
