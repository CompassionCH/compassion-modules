<?xml version="1.0" ?>
<odoo>
    <record id="support_request_reminder_pref" model="ir.ui.view">
        <field name="name">supp.request.reminder</field>
        <field name="model">res.users</field>
        <field name="inherit_id" ref="base.view_users_form_simple_modif" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='email']" position="after">
                <field name="do_reminder_support_req" />
            </xpath>
        </field>
    </record>
</odoo>
