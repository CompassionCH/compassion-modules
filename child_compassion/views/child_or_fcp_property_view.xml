<odoo>
    <record id="view_child_property_form" model="ir.ui.view">
        <field name="name">child.property.form</field>
        <field name="model">child.property</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name" readonly="1"/>
                        <field name="value"/>
                    </group>
                </sheet>
                <div class="oe_chatter">
                    <field name="message_follower_ids"/>
                    <field name="activity_ids"/>
                    <field name="message_ids"/>
                </div>
            </form>
        </field>
    </record>

    <record id="view_household_duty_form" model="ir.ui.view">
        <field name="name">household.duty.form</field>
        <field name="model">child.household.duty</field>
        <field name="inherit_id" ref="view_child_property_form"/>
        <field name="mode">primary</field>
        <field name="arch" type="xml">
            <form/>
        </field>
    </record>
