"${child.preferred_name} / ${child.project_id.country_id.name}\n"

#. module: mobile_app_connector
#: model:mobile.app.tile.subtype,default_body:mobile_app_connector.tile_subtype_gi5
msgid ""
"% set partner = ctx['objects'].sudo()\n"
"% set invoice = partner.env['account.invoice'].search([('partner_id', '=', "
"partner.id), ('state', '=', 'paid'), ('invoice_type', '!=', 'sponsorship')], "
"order='last_payment desc', limit=1)\n"
"% if invoice:\n"
"You gave CHF ${invoice.amount_total} on ${invoice.get_date('last_payment')}\n"
"% else:\n"
"Make a donation"
msgstr ""
"% set partner = ctx['objects'].sudo()\n"
"% set invoice = partner.env['account.invoice'].search([('partner_id', '=', "
"partner.id), ('state', '=', 'paid'), ('invoice_type', '!=', 'sponsorship')], "
"order='last_payment desc', limit=1)\n"
"% if invoice:\n"
"Vous avez donné CHF ${invoice.amount_total} le ${invoice."
"get_date('last_payment')}\n"
"% else:\n"
"Faire un don"

#. module: mobile_app_connector
#: model:mobile.app.tile,body:mobile_app_connector.tile_le1
#: model:mobile.app.tile.subtype,default_body:mobile_app_connector.tile_subtype_LE1
msgid ""
"% set sponsorship = ctx['objects'].sudo()\n"
"% set letters = sponsorship.sponsor_letter_ids\n"
"% if sponsorship.last_letter == 0:\n"
"You wrote today.\n"
"% else:\n"
"    % if sponsorship.last_letter > 0:\n"
"You last wrote ${sponsorship.last_letter} days ago\n"
"    % else:\n"
"You never wrote to your sponsored child.\n"
"    % endif\n"
"% endif"
msgstr ""
"% set sponsorship = ctx['objects'].sudo()\n"
"% set letters = sponsorship.sponsor_letter_ids\n"
"% if sponsorship.last_letter == 0:\n"
"Vous avez écrit aujourd'hui\n"
"% else:\n"
"    % if sponsorship.last_letter > 0:\n"
"Cela fait ${sponsorship.last_letter} jours que vous avez écrit\n"
"    % else:\n"
"Vous n'avez encore jamais écrit\n"
"    % endif\n"
"% endif"

