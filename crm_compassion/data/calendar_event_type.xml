<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!--
            Resource: calendar.event.type
        -->
        <record id="calendar_event" model="calendar.event.type">
            <field name="name">Event</field>
        </record>

        <!-- Deactivate e-mails for calendar events -->
        <record id="calendar_no_email" model="ir.config_parameter">
            <field name="key">calendar.block_mail</field>
            <field name="value">True</field>
        </record>
    </data>
</odoo>
