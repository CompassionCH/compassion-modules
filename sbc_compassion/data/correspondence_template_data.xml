<openerp>
  <data noupdate="1">
    <!-- Templates -->
    <record id="default_template" model="sponsorship.correspondence.template">
      <field name="name">Default Template</field>
      <field name="layout">CH-A-6S01-1</field>
      <field name="checkbox_ids" eval="False"/>
    </record>

    <record id="threshold_keypoints_template" model="ir.config_parameter">
      <field name="key">threshold_keypoints_template</field>
      <field name="value">0.4</field>
    </record>

    <!-- QR Code -->
    <record id="qrcode_x_min" model="ir.config_parameter">
      <field name="key">qrcode_x_min</field>
      <field name="value">0.0</field>
    </record>
    
    <record id="qrcode_x_max" model="ir.config_parameter">
      <field name="key">qrcode_x_max</field>
      <field name="value">0.6</field>
    </record>
    
    <record id="qrcode_y_min" model="ir.config_parameter">
      <field name="key">qrcode_y_min</field>
      <field name="value">0.0</field>
    </record>
    
    <record id="qrcode_y_max" model="ir.config_parameter">
      <field name="key">qrcode_y_max</field>
      <field name="value">0.15</field>
    </record>

    <record id="bluecorner_y_max" model="ir.config_parameter">
      <field name="key">bluecorner_y_max</field>
      <field name="value">0.15</field>
    </record>

    <record id="qrcode_x_min" model="ir.config_parameter">
      <field name="key">bluecorner_x_min</field>
      <field name="value">0.7</field>
    </record>
    
    <!-- UPDATE MICHAEL SANDOZ 02.2016
        folder paths imported scan letter and treatment done scan letter
    -->
    <record id="sbc_compassion.share_on_nas" model="ir.config_parameter">
      <field name="key">sbc_compassion.share_on_nas</field>
      <field name="value">Public</field>
    </record>
    
    <record id="sbc_compassion.scan_letter_imports" model="ir.config_parameter">
      <field name="key">sbc_compassion.scan_letter_imported</field>
      <field name="value">/Scan Lettres S2B/Imports/</field>
    </record>
    
    <record id="sbc_compassion.scan_letter_done" model="ir.config_parameter">
      <field name="key">sbc_compassion.scan_letter_done</field>
      <field name="value">/Scan Lettres S2B/Done/</field>
    </record>
  </data>
</openerp>
