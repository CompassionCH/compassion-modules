<odoo>
    <!-- S2B Templates -->
    <record id="default_template" model="correspondence.template">
        <field name="name">Default Template</field>
        <field name="layout">CH-A-6S11-1</field>
    </record>

    <!-- S2B text boxes -->
    <record id="s2b_header_box" model="correspondence.text.box">
        <field name="name">S2B-default header</field>
        <field name="x_min">5</field>
        <field name="x_max">170</field>
        <field name="y_min">8</field>
        <field name="y_max">30</field>
        <field name="text_line_height">4</field>
    </record>
    <record id="s2b_checkbox_fr" model="correspondence.lang.checkbox">
        <field name="name">French checkbox</field>
        <field name="language_id" ref="child_compassion.lang_compassion_french"/>
        <field name="x_min">920</field>
        <field name="x_max">1050</field>
        <field name="y_min">60</field>
        <field name="y_max">150</field>
    </record>
    <record id="s2b_checkbox_en" model="correspondence.lang.checkbox">
        <field name="name">English checkbox</field>
        <field name="language_id" ref="child_compassion.lang_compassion_english"/>
        <field name="x_min">1540</field>
        <field name="x_max">1680</field>
        <field name="y_min">60</field>
        <field name="y_max">150</field>
    </record>
    <record id="s2b_checkbox_es" model="correspondence.lang.checkbox">
        <field name="name">Spanish checkbox</field>
        <field name="language_id" ref="child_compassion.lang_compassion_spanish"/>
        <field name="x_min">920</field>
        <field name="x_max">1050</field>
        <field name="y_min">190</field>
        <field name="y_max">300</field>
    </record>
    <record id="s2b_checkbox_po" model="correspondence.lang.checkbox">
        <field name="name">Portuguese checkbox</field>
        <field name="language_id" ref="child_compassion.lang_compassion_portuguese"/>
        <field name="x_min">920</field>
        <field name="x_max">1050</field>
        <field name="y_min">310</field>
        <field name="y_max">420</field>
    </record>

    <record id="s2b_l1_textbox_original" model="correspondence.text.box">
        <field name="name">S2B-L1-1-1</field>
        <field name="x_min">15</field>
        <field name="x_max">98</field>
        <field name="y_min">70</field>
        <field name="y_max">230</field>
        <field name="text_line_height">5.32</field>
        <field name="text_type">Original</field>
    </record>
    <record id="s2b_l1_textbox_original2" model="correspondence.text.box">
        <field name="name">S2B-L1-2-1</field>
        <field name="x_min">15</field>
        <field name="x_max">101</field>
        <field name="y_min">21</field>
        <field name="y_max">255</field>
        <field name="text_line_height">7.2</field>
        <field name="text_type">Original</field>
    </record>

    <record id="s2b_l2_design" model="correspondence.positioned.object">
        <field name="name">S2B-L2-1-1</field>
        <field name="x_min">15</field>
        <field name="x_max">184</field>
        <field name="y_min">15</field>
        <field name="y_max">270</field>
    </record>
    <record id="s2b_l2_textbox_original" model="correspondence.text.box">
        <field name="name">S2B-L2-2-1</field>
        <field name="x_min">15</field>
        <field name="x_max">200</field>
        <field name="y_min">55</field>
        <field name="y_max">165</field>
        <field name="text_line_height">5.32</field>
        <field name="text_type">Original</field>
    </record>

    <record id="s2b_l3_design" model="correspondence.positioned.object">
        <field name="name">S2B-L3-1-1</field>
        <field name="x_min">15</field>
        <field name="x_max">184</field>
        <field name="y_min">15</field>
        <field name="y_max">270</field>
    </record>
    <record id="s2b_l3_textbox_original" model="correspondence.text.box">
        <field name="name">S2B-L3-2-1</field>
        <field name="x_min">15</field>
        <field name="x_max">185</field>
        <field name="y_min">55</field>
        <field name="y_max">152</field>
        <field name="text_line_height">5.32</field>
        <field name="text_type">Original</field>
    </record>

    <record id="s2b_l4_design" model="correspondence.positioned.object">
        <field name="name">S2B-L4-1-1</field>
        <field name="x_min">15</field>
        <field name="x_max">184</field>
        <field name="y_min">15</field>
        <field name="y_max">270</field>
    </record>
    <record id="s2b_l4_design_2" model="correspondence.positioned.object">
        <field name="name">S2B-L4-2-1</field>
        <field name="x_min">15</field>
        <field name="x_max">113</field>
        <field name="y_min">55</field>
        <field name="y_max">270</field>
    </record>
    <record id="s2b_l4_textbox_original" model="correspondence.text.box">
        <field name="name">S2B-L4-2-2</field>
        <field name="x_min">115</field>
        <field name="x_max">192</field>
        <field name="y_min">52</field>
        <field name="y_max">148</field>
        <field name="text_line_height">5</field>
        <field name="text_type">Original</field>
    </record>

    <record id="s2b_l5_textbox_original" model="correspondence.text.box">
        <field name="name">S2B-L5-1-1</field>
        <field name="x_min">15</field>
        <field name="x_max">200</field>
        <field name="y_min">75</field>
        <field name="y_max">257</field>
        <field name="text_line_height">5.32</field>
        <field name="text_type">Original</field>
    </record>

    <record id="s2b_l6_textbox_original" model="correspondence.text.box">
        <field name="name">S2B-L6-1-1</field>
        <field name="x_min">15</field>
        <field name="x_max">184</field>
        <field name="y_min">50</field>
        <field name="y_max">270</field>
        <field name="text_line_height">5.32</field>
        <field name="text_type">Original</field>
    </record>
    <record id="s2b_l6_textbox_translation" model="correspondence.text.box">
        <field name="name">S2B-L6-2-1</field>
        <field name="x_min">15</field>
        <field name="x_max">190</field>
        <field name="y_min">15</field>
        <field name="y_max">290</field>
        <field name="text_line_height">4</field>
        <field name="text_type">Translation</field>
    </record>

    <record id="threshold_keypoints_template" model="ir.config_parameter">
        <field name="key">threshold_keypoints_template</field>
        <field name="value">0.4</field>
    </record>

    <!-- QR Code settings -->
    <record id="qrcode_x_min" model="ir.config_parameter">
        <field name="key">qrcode_x_min</field>
        <field name="value">0.0</field>
    </record>
    <record id="qrcode_x_max" model="ir.config_parameter">
        <field name="key">qrcode_x_max</field>
        <field name="value">0.42</field>
    </record>
    <record id="qrcode_y_min" model="ir.config_parameter">
        <field name="key">qrcode_y_min</field>
        <field name="value">0.0</field>
    </record>
    <record id="qrcode_y_max" model="ir.config_parameter">
        <field name="key">qrcode_y_max</field>
        <field name="value">0.3</field>
    </record>

    <!-- Blue corner settings -->
    <record id="bluecorner_y_max" model="ir.config_parameter">
        <field name="key">bluecorner_y_max</field>
        <field name="value">0.07</field>
    </record>
    <record id="bluecorner_x_min" model="ir.config_parameter">
        <field name="key">bluecorner_x_min</field>
        <field name="value">0.9</field>
    </record>
</odoo>
